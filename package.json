{
  "name": "react-component-boilerplate",
  "version": "2.2.7",
  "keywords": [
    "react",
    "component",
    "boilerplate"
  ],
  "main": "dist/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/react-component-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "esca-scripts build",
    "bundle": "esca-scripts bundle --src src/inject.js --dist dist-bundle",
    "dev": "esca-scripts dev",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "serve": "esca-scripts serve --src dist-test",
    "test": "npm run build && npm run test:bundle && npm run test:run",
    "test:bundle": "esca-scripts bundle --src test/wrapper/index.html --dist dist-test",
    "test:run": "esca-scripts test",
    "test:serve": "npm run build && npm run test:bundle && esca-scripts serve --src dist-test",
    "travis": "opn https://travis-ci.org/escaladesports/react-component-boilerplate/branches"
  },
  "devDependencies": {
    "esca-scripts": "^1.4.0",
    "exec-cmd": "^2.0.1",
    "fs-extra": "^6.0.0",
    "get-port": "^3.2.0",
    "opn-cli": "^3.1.0",
    "puppeteer": "^1.4.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "static-server": "^2.2.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
